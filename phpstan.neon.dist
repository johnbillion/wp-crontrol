includes:
    - vendor/szepeviktor/phpstan-wordpress/extension.neon
parameters:
    level: 3
    inferPrivatePropertyTypeFromConstructor: true
    paths:
        - wp-crontrol.php
        - src/
    bootstrapFiles:
        - tests/stubs.php
    ignoreErrors:
        # Uses func_get_args()
        - '#^Function apply_filters invoked with [34567] parameters, 2 required\.$#'
		# External plugin
        - '#QM_Util#'
